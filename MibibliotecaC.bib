@article{simonsen_measuring_2003,
	title = {Measuring anti-correlations in the nordic electricity spot market by wavelets},
	volume = {322},
	issn = {0378-4371},
	url = {http://www.sciencedirect.com/science/article/pii/S0378437102019386},
	doi = {10.1016/S0378-4371(02)01938-6},
	abstract = {We consider the Nordic electricity spot market from mid-1992 to the end of year 2000. This market is found to be well approximated by an anti-persistent self-affine (mean-reverting) walk. It is characterized by a Hurst exponent of H≃0.41 over three orders of magnitude in time ranging from days to years. We argue that in order to see such a good scaling behavior, and to locate cross-overs, it is crucial that an analyzing technique is used that decouples scales. This is in our case achieved by utilizing a (multi-scale) wavelet approach. The shortcomings of methods that do not decouple scales are illustrated by applying, to the same data set, the classic R/S- and Fourier techniques, for which scaling regimes and/or positions of cross-overs are hard to define.},
	language = {en},
	urldate = {2020-04-20},
	journal = {Physica A: Statistical Mechanics and its Applications},
	author = {Simonsen, Ingve},
	month = may,
	year = {2003},
	keywords = {Anti-correlation, Econophysics, Measuring Hurst exponents, Self-affine, Wavelet transform},
	pages = {597--606},
	file = {ScienceDirect Snapshot:/home/victor/Zotero/storage/3ZE8J9NA/S0378437102019386.html:text/html;Versión enviada:/home/victor/Zotero/storage/Q9MABLWC/Simonsen - 2003 - Measuring anti-correlations in the nordic electric.pdf:application/pdf}
}

